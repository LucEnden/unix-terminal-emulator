{
	"name": "unix-terminal-emulator",
	"version": "2.1.0",
	"description": "Emulates a Unix terminal inside the DOM!",
	"keywords": [
		"unix",
		"terminal",
		"emulator",
		"unix terminal emulator"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/LucEnden/unix-terminal-emulator"
	},
	"author": "Luc van den Enden",
	"license": "MIT",
	"type": "commonjs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"serve": "webpack-dev-server",
		"build:dev": "webpack --config ./webpack.config.js --mode=development --node-env=development --stats-error-details && tsc --emitDeclarationOnly",
		"build:prod": "webpack --config ./webpack.config.js --mode=production --node-env=production --stats-error-details && tsc --emitDeclarationOnly",
		"test": "jest",
		"document": "npx typedoc --out docs --excludePrivate --readme none --theme github-wiki --entryPointStrategy expand ./src"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@babel/preset-env": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@types/graphlib": "^2.1.8",
		"@types/jest": "^28.1.5",
		"@types/node": "^18.0.3",
		"babel-jest": "^28.1.3",
		"babel-loader": "^8.2.5",
		"css-loader": "^6.7.1",
		"graphlib": "^2.1.8",
		"html-entities": "^2.3.3",
		"jest-environment-jsdom": "^28.1.2",
		"nanoid": "^3.3.4",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.3.3",
		"text-encoding": "^0.7.0",
		"ts-jest": "^28.0.6",
		"ts-node": "^10.9.1",
		"typedoc": "^0.23.10",
		"typedoc-github-wiki-theme": "^1.0.1",
		"typedoc-plugin-markdown": "^3.13.4",
		"typedoc-plugin-rename-defaults": "^0.6.4",
		"util": "^0.12.4",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0"
	}
}
